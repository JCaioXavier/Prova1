import java.util.Scanner;

public class Main{
    public static void main(String[] args){

        Scanner scan = new Scanner(System.in);

        System.out.println("Digite um valor inteiro: ");
        int num = scan.nextInt();

        for(int i = 1; i <= 10; i++){
            System.out.printf("%d * %d = %d\n", i, num, num * i);
        }
    }
}

